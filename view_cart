      item_brand: 'TRU RED™',			// insert an actual product price
      item_category: 'Paper',			// insert an actual product top-level category
	    item_category2: 'Notebooks',		// if it is possible to drill down the categories (e.g. Apparel, then T-shirt, then Men), use item_category2, item_category3, etc. Can use from item_category up to item_category
      item_category3: 'Single and Multi Subject Notebooks',
      item_variant: 'Brown',
